{
  "author": "YOU <YOU@example.com>",
  "name": "my_actionhero_project",
  "description": "my actionhero project",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@types/ws": "^8.5.12",
    "actionhero": "29.3.2",
    "ioredis": "latest",
    "ioredis-mock": "latest",
    "winston": "latest",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.5",
    "@types/glob": "latest",
    "@types/ioredis-mock": "^8.2.5",
    "@types/jest": "latest",
    "@types/node": "latest",
    "jest": "latest",
    "prettier": "^3.3.3",
    "ts-jest": "latest",
    "ts-node-dev": "latest",
    "type-fest": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "todo": "ts-node-dev TODO: should be removed due to deprication",
    "postinstall": "npm run build",
    "dev": "ts-node-dev --no-deps --transpile-only ./src/server",
    "debug": "tsc && node --inspect-brk -r ts-node/register --require source-map-support/register ./src/server.ts",
    "start": "node ./dist/server.js",
    "actionhero": "actionhero",
    "test": "jest",
    "pretest": "npm run build && npm run lint",
    "build": "tsc --sourceMap true --declaration",
    "format": "prettier --write \"**/*.{js,ts,json}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json}\"",
    "debug:inspect": "node --inspect -r ts-node/register ./src/server.ts",
    "debug:break": "node --inspect-brk -r ts-node/register ./src/server.ts"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  }
}
